alias dest='eval $(sets.sh dest); tail -1 ~/links/.gets'
alias here='eval $(sets.sh here); tail -1 ~/links/.gets'
alias sets='eval $(sets.sh); tail -1 ~/links/.gets'
alias src='eval $(sets.sh src); tail -1 ~/links/.gets'
alias gets='. ~/links/.gets'
